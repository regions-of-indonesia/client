{
  "name": "@regions-of-indonesia/client",
  "version": "5.2.0",
  "description": "Regions of Indonesia JavaScript client",
  "keywords": [
    "regions-of-indonesia"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./lite": {
      "import": {
        "types": "./dist/lite.d.ts",
        "default": "./dist/lite.js"
      },
      "require": {
        "types": "./dist/lite.d.ts",
        "default": "./dist/lite.cjs"
      }
    }
  },
  "files": [
    "dist",
    "lite.d.ts"
  ],
  "private": false,
  "repository": "https://github.com/regions-of-indonesia/client.git",
  "author": {
    "name": "flamrdevs",
    "url": "https://github.com/flamrdevs"
  },
  "license": "GPL-3.0",
  "scripts": {
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build": "vite build",
    "release": "pnpm build && pnpm test && release-it"
  },
  "dependencies": {
    "@regions-of-indonesia/types": "5.1.1"
  },
  "devDependencies": {
    "@regions-of-indonesia/utils": "5.1.1",
    "@vitest/ui": "^0.34.4",
    "node-fetch": "^3.3.2",
    "release-it": "^16.1.5",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.4"
  }
}
